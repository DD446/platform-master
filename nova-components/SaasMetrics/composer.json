{
    "name": "podcaster/saas-metrics",
    "description": "podcaster metrics",
    "keywords": [
        "laravel",
        "nova",
        "podcaster",
        "metrics"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\SaasMetrics\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\SaasMetrics\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
