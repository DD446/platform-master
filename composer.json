{
    "name": "podcaster/api",
    "description": "podcaster.de API",
    "keywords": [
        "podcaster",
        "podcast"
    ],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "./nova-components/FundsManagement"
        },
        {
            "type": "path",
            "url": "./nova-components/PioniereManagement"
        },
        {
            "type": "path",
            "url": "./nova-components/SaasMetrics"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mongodb": "*",
        "ext-soap": "*",
        "ext-zip": "*",
        "ext-zlib": "*",
        "abraham/twitteroauth": "^3.3.0",
        "alfrasc/laravel-matomo-tracker": "^1.0",
        "anhskohbo/no-captcha": "^3.4",
        "artesaos/seotools": "^v0.20.2",
        "audiotakes/pioniere-management": "*",
        "axy/htpasswd": "^1.0",
        "babenkoivan/elastic-adapter": "^2.4",
        "babenkoivan/elastic-scout-driver": "^2.0",
        "babenkoivan/elastic-scout-driver-plus": "^3.5",
        "barryvdh/laravel-dompdf": "^v0.9.0",
        "beyondcode/laravel-websockets": "1.13.1",
        "binarybuilds/nova-advanced-command-runner": "^3.0",
        "danielebarbaro/laravel-vat-eu-validator": "^0.5.4",
        "ddeboer/vatin": "^2.2",
        "diglactic/laravel-breadcrumbs": "^v6.1",
        "dougsisk/laravel-country-state": "^4.1",
        "dyrynda/laravel-efficient-uuid": "^4.5",
        "dyrynda/laravel-model-uuid": "^6.6",
        "ebess/advanced-nova-media-library": "^3.8",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.2",
        "gregoriohc/laravel-nova-theme-responsive": "^0.8",
        "intervention/image": "^2.7",
        "intervention/imagecache": "^2.5",
        "intervention/validation": "^3.2",
        "james-heinrich/getid3": "^2.0",
        "jenssegers/mongodb": "^3.8",
        "jgrossi/corcel": "^5.1",
        "jorenvanhocht/laravel-share": "^3.3",
        "kabbouchi/nova-impersonate": "^v1.14.0",
        "khalin/nova-link-field": "^1.2",
        "khill/php-duration": "^1.1",
        "laminas/laminas-cache": "2.13.2",
        "laminas/laminas-feed": "^2.18",
        "laminas/laminas-http": "^2.16",
        "laminas/laminas-twitter": "^3.4.0",
        "laminas/laminas-validator": "^2.23.0",
        "laravel-validation-rules/credit-card": "^1.7.0",
        "laravel/fortify": "^1.13",
        "laravel/framework": "^v8.83.23",
        "laravel/horizon": "^v5.10",
        "laravel/nova": "3.32.*",
        "laravel/passport": "^v10.4.1",
        "laravel/socialite": "^v5.5.5",
        "league/oauth1-client": "^v1.10.1",
        "league/oauth2-client": "^2.6",
        "librenms/laravel-vue-i18n-generator": "^0.1.47",
        "manogi/nova-tiptap": "^v2.8.3",
        "matomo/device-detector": "^5.0",
        "monarobase/country-list": "^3.3",
        "pbmedia/laravel-ffmpeg": "^7.8",
        "pion/laravel-chunk-upload": "^v1.5.0",
        "podcaster/funds-management": "*",
        "podcaster/saas-metrics": "*",
        "podcasthosting/auphonic-client": "^0.8.1",
        "podcasthosting/podcast-client-spotify": "^0.12.5",
        "pusher/pusher-php-server": "7.0.2",
        "rcrowe/twigbridge": "^0.12",
        "rocramer/nova-matomo-analytics": "^0.5.0",
        "socialiteproviders/twitter": "^4.1",
        "spatie/browsershot": "^3.57",
        "spatie/eloquent-sortable": "^3.11",
        "spatie/laravel-backup": "^6.16",
        "spatie/laravel-cookie-consent": "^2.12",
        "spatie/laravel-failed-job-monitor": "^3.4",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-json-api-paginate": "^1.12",
        "spatie/laravel-medialibrary": "^8.10",
        "spatie/laravel-sluggable": "^2.6",
        "spatie/laravel-webhook-client": "^2.8.0",
        "spiritix/lada-cache": "^5.2",
        "stevebauman/location": "^v6.5.0",
        "teamtnt/laravel-scout-tntsearch-driver": "v11.5.0",
        "titasgailius/search-relations": "^1.0",
        "titasgailius/terminal": "^0.4",
        "umpirsky/language-list": "^1.1",
        "usmanhalalit/laracsv": "^2.1",
        "visualappeal/matomo-php-api": "^1.6",
        "webpatser/laravel-countries": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^v3.7",
        "beyondcode/laravel-dump-server": "^1.8.0",
        "beyondcode/laravel-query-detector": "^1.6.0",
        "facade/ignition": "^2.17.6",
        "filp/whoops": "^2.14.5",
        "kitloong/laravel-migrations-generator": "^v4.5.1",
        "knuckleswtf/scribe": "^3.37.0",
        "laravel/telescope": "^v4.9.2",
        "laravel/tinker": "^v2.7.2",
        "mockery/mockery": "^1.5.0",
        "nunomaduro/collision": "^v5.11.0",
        "phpunit/phpunit": "^9.5",
        "reliese/laravel": "^1.1",
        "sven/artisan-view": "3.4"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan nova:publish",
            "@php artisan horizon:publish"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php":"7.4"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
